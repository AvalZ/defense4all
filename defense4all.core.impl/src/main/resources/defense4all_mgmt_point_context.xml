<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
	   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="mgmtPointImpl" 
		class="org.opendaylight.defense4all.core.impl.DFMgmtPointImpl">
		<property name="frameworkMain" ref="frameworkMain" />
		<property name="dfAppRoot" ref="appRoot" />
		
		<!-- default OFC settings -->

		<property name="ofcHostname" value="odl_controller" />
		<property name="ofcUsername" value="admin" />
		<property name="ofcPassword" value="admin" />

		<property name="ofcPort" value="8080" />
		<property name="ofcForStatsCollection" value="true" />
		<property name="ofcForDvsn" value="true" />
		<property name="ofcSetInSpring" value="false" />

		<!-- default OFS settings -->

		<property name="ofsLabel" value="ovs" />
		<property name="ofsId" value="00:00:00:50:56:a3:1b:80" />
		<property name="ofsType" value="OF" />
		<property name="ofsMgmtAddr" value="10.206.102.78" />
		<property name="ofsMgmtPort" value="0" />
		<property name="ofsSdnNodeMode" value="SDN_ENABLED_NATIVE" />
		<property name="ofsHealthFrequency" value="120" />

		<property name="ofsAmsConnAmsLabel" value="dp1" />
		<property name="ofsAmsConnNetNodeNorthPort" value="2" />
		<property name="ofsAmsConnNetNodeSouthPort" value="4" />
		<property name="ofsAmsConnAmsNorthPort" value="1" />
		<property name="ofsAmsConnAmsSouthPort" value="2" />

		<property name="ofsTrafficNorthPortLabel" value="north1" />
		<property name="ofsTrafficNorthPort" value="1" />
		<property name="ofsTrafficNorthPortVlan" value="0" />
		<property name="ofsTrafficNorthPortLocation" value="NORTH" />

		<property name="ofsTrafficSouthPortLabel" value="south1" />
		<property name="ofsTrafficSouthPort" value="3" />
		<property name="ofsTrafficSouthPortVlan" value="0" />
		<property name="ofsTrafficSouthPortLocation" value="SOUTH" />

		<property name="ofsProtectedLinkLabel" value="1_to_3" />
		<property name="ofsProtectedLinkInPort" value="1" />
		<property name="ofsProtectedLinkOutPort" value="3" />

		<property name="ofsSetInSpring" value="false" />
		
		<!-- default AMS settings -->
		<property name="amsLabel" value="dp1" />
		<property name="amsForStatsCollection" value="false" />
		<property name="amsForDvsn" value="true" />
		<property name="amsUsername" value="radware" />
		<property name="amsPassword" value="radware" />
		<property name="amsSecurityConfigKeys" value="" />
		<property name="amsMgmtAddr" value="10.206.167.50" />
		<property name="amsSetInSpring" value="false" />
	</bean>
</beans>	
